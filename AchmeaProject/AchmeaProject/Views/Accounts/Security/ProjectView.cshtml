@model List<ProjectViewModel>
@{
    ViewData["Title"] = "View";
    Layout = "~/Views/Shared/_SecurityLayout.cshtml";
}

<table class="table table-hover">
    <thead>
        <tr>
            <th>ProjectId</th>
            <th>Status</th>
            <th>Employees</th>
        </tr>
    </thead>
    <tbody>
        @foreach (ProjectViewModel P in Model)
        {
        <tr onclick="window.location.href='@Url.Action("Details", "Security", new { id = P.ProjectId})'">
            <td>@P.ProjectId</td>
            <td>@P.Status</td>
        </tr>
        }
    </tbody>
</table>